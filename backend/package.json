{
  "name": "edify-tech-backend",
  "version": "0.0.1",
  "description": "Edify Tech Application",
  "author": "Edify Tech",
  "contributors": [
    "Vin√≠cius G. Oliveira"
  ],
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register ./src/main.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:staged": "eslint --fix",
    "test:staged": "yarn test --findRelatedTests",
    "test": "jest --passWithNoTests --runInBand --no-cache --detectOpenHandles --forceExit",
    "test:unit": "yarn test --config ./jest-unit.config.js",
    "test:verbose": "jest --verbose",
    "test:watch": "yarn test --watch --config ./jest-unit.config.js",
    "test:cov": "yarn test --coverage --config ./jest-unit.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --passWithNoTests --runInBand --no-cache --config ./jest-unit.config.js",
    "test:e2e": "yarn test --config ./jest-e2e.config.js",
    "snyk": "snyk test",
    "release": "standard-version",
    "changelog:minor": "standard-version --release-as minor && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:major": "standard-version --release-as major && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:patch": "standard-version --release-as patch && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:alpha": "standard-version --prerelease alpha && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "prisma:generate": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:only": "prisma migrate dev --create-only",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "start:migrate:dev": "prisma migrate deploy && npm run start:dev"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/platform-express": "^10.3.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^10.0.0",
    "@prisma/client": "^5.3.1",
    "@prisma/instrumentation": "^5.7.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "fs-extra": "^11.1.1",
    "joi": "^17.9.2",
    "axios": "^1.4.0",
    "nest-winston": "^1.9.2",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "winston": "^3.9.0",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.7",
    "@golevelup/nestjs-testing": "^0.1.2",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^10.1.3",
    "@types/express": "^4.17.17",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "auto-changelog": "^2.4.0",
    "commitlint": "^17.6.7",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "faker": "^6.6.6",
    "faker-br": "^0.4.1",
    "git-commit-msg-linter": "^5.0.4",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "prisma": "^5.6.0",
    "snyk": "^1.1196.0",
    "source-map-support": "^0.5.21",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "standard-version": {
    "skip": {
      "changelog": true,
      "commit": true
    }
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  }
}